<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="424.0" prefWidth="571.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.startPage">
    <children>
        <ImageView fitHeight="430.0" fitWidth="571.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Image/photo_2021-04-23_10-38-51.jpg" />
            </image>
        </ImageView>
        <Button fx:id="start" layoutX="227.0" layoutY="178.0" mnemonicParsing="false" onAction="#startPressed" prefHeight="44.0" prefWidth="100.0" text="Start">
            <font>
                <Font name="System Bold Italic" size="18.0" />
            </font>
        </Button>
        <Button fx:id="logout" layoutX="227.0" layoutY="230.0" mnemonicParsing="false" onAction="#logoutPressed" prefHeight="44.0" prefWidth="100.0" text="Log out">
            <font>
                <Font name="System Bold Italic" size="18.0" />
            </font>
        </Button>
        <Button fx:id="exit" layoutX="227.0" layoutY="282.0" mnemonicParsing="false" onAction="#exitPressed" prefHeight="44.0" prefWidth="100.0" text="Exit">
            <font>
                <Font name="System Bold Italic" size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
